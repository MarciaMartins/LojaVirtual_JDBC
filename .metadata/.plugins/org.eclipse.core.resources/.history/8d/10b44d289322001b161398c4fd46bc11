package lojaVirtual_JDBC.modelo;

public class Categoria {
	private Integer id;
	private String descricao;
	
	public Categoria(Integer id, String descricao){
		this.id = id;
		this.descricao = descricao;
	}
	
	public String getDescricao() {
		return descricao;
	}
	
	public Integer getId() {
		return id;
	}
}
